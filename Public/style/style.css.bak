/* ######################### */
/* ##### user-defined ###### */
/* ######################### */
body { font-family:"Arial","Helvetica","sans-serif","Verdana","����"; }

a { color:#004080; text-decoration:none; }			/*,a:link,a:visited*/
a:hover { color:#D20000; text-decoration:underline; }		/*,a:active*/


/* ######################### */
/* ########## Page ######### */
/* ######################### */
div.Framer { clear:both; width:980px; }
div.Framers,
div.FramerFloor { clear:both; width:980px; text-align:left; }
div.FramerFloor div.MainFull { width:980px; }
div.FramerFloor div.MainBody,
div.FramerFloor div.MainContent { float:left; width:10%; }
div.FramerFloor div.MainCenter { float:left; width:50%px; }
div.FramerFloor div.MainSide,
div.FramerFloor div.MainMenu,
div.FramerFloor div.SideLeft { float:left; width:230px; margin:0 8px 0 0; }
div.FramerFloor div.SideRight { float:right; width:230px; margin:0 0 0 8px; }
div.FramerFloor div.MainRelate { float:left; width:230px; margin:0 0 0 8px; }
div.FramerFloor .widFull { width:980px; }
div.FramerFloor .widBody { width:742px; }
div.FramerFloor .widCenter { width:494px; }
div.FramerFloor .widLeft,
div.FramerFloor .widRight { width:230px; }

div.FramerFloor div.MainHalfCenter { float:left; width:360px; }
div.FramerFloor div.MainHalfLeft { float:left; width:359px; margin:0 8px 0 0; }
div.FramerFloor div.MainHalfRight { float:right; width:360px; margin:0 0 0 8px; }

div.FramerFloor div.MainFrame { clear:both; }
div.FramerFloor div.MainFrame h2,
div.FramerFloor div.MainFrame h3,
div.FramerFloor div.MainFrame h2 a,
div.FramerFloor div.MainFrame h3 a { color:#013E55; }
div.FramerFloor div.MainFrame h2 a:hover,
div.FramerFloor div.MainFrame h3 a:hover { color:#D20000; }
div.FramerFloor div.MainFrame div.title,
div.FramerFloor div.MainFrame div.subtitle,
div.FramerFloor div.MainFrame div.topic { color:#29458C; }
div.FramerFloor div.MainFrame div.subtopic { color:#235D83; }
div.FramerFloor div.MainFrame div.h1 { color:#235D83; }
div.FramerFloor div.MainFrame div.h1 a { color:#EC5F00; }
div.FramerFloor div.MainFrame div.h1 a:hover { color:#FF0000; }

/* ##### FrameSideMenu ##### */
div.FrameSideMenu { height:400px; background:url("../images/menu_bg.gif") repeat-x; }
div.FrameSideMenu div.partFrame { height:400px; background:url("../images/menu_rightline.gif") top right no-repeat; }
div.FrameSideMenu div.partBody {  }
div.FrameSideMenu div.partBar { height:25px; line-height:25px; text-align:left; background:url("../images/menu_bg_bar.gif") top left no-repeat; }
div.FrameSideMenu div.partBar a { display:block; font-size:12px; font-weight:bold; padding-left:20px; }
div.FrameSideMenu div.partBar a,
div.FrameSideMenu div.partBar a:visited { color:#FFFFFF; text-decoration:none; }
div.FrameSideMenu div.partBar a:hover,
div.FrameSideMenu div.partBar a:active { color:#FFFFFF; text-decoration:underline; }
div.FrameSideMenu div.partContent {  }
div.FrameSideMenu div.partContent ul {  }
div.FrameSideMenu div.partContent ul li { text-align:left; }
div.FrameSideMenu div.partContent ul li a { display:block; height:25px; line-height:25px; background:url("../images/menu_bg_item.gif") top left no-repeat; font-size:12px; font-weight:bold; padding-left:20px; }
div.FrameSideMenu div.partContent ul li a,
div.FrameSideMenu div.partContent ul li a:visited { color:#333333; text-decoration:none; }
div.FrameSideMenu div.partContent ul li a:hover,
div.FrameSideMenu div.partContent ul li a:active { color:#333333; text-decoration:none; background:url("../images/menu_bg_items.gif") top left no-repeat; }
div.FrameSideMenu div.partContent ul li.pop a { background:url("../images/menu_bg_items.gif") top left no-repeat; }


/* ######### Header ######## */
#Confine { width:980px; height:0; overflow:hidden; }
#Header { width:980px; overflow:hidden; }
div.HeaderSpace { clear:both; margin:2px; padding:2px; }
div.HeaderTopSpace { height:10px; overflow:hidden; }

#HeaderFrame { clear:both; background:url("../images/header_frame_bg.gif") repeat-x; }
#HeaderFrameInner {  }
#HeaderFrameBody { padding:5px 0; }
/*#HeaderLogo { float:left; width:300px; height:50px; text-align:center; }
#HeaderLogoFrame { padding:0; }*/
#logo-header-link { display:inline-block; width:150px; height:50px; background:url("../images/logo_header.gif") 0 0 no-repeat; text-decoration:none!important; }
#logo-extend-link { display:inline-block; width:120px; height:50px; background:url("../images/logo_extend.gif") 0 0 no-repeat; text-decoration:none!important; }

#HeaderSubMenu { float:right; height:20px; line-height:20px; margin:18px 1px 0 0; padding:0; }
#HeaderSubMenu div.partFrame {  }
#HeaderSubMenu div.partBody {  }
#HeaderSubMenu ul {  }
#HeaderSubMenu li { display:inline; height:18px;line-height:18px; }
#HeaderSubMenu li span {  }
#HeaderSubMenu li.space { padding:0 10px 0 10px; }
#HeaderSubMenu li a { float:left; display:block; border:0; margin:0 0 0 2px; padding:2px 6px 0 6px; }
#HeaderSubMenu li a,
#HeaderSubMenu li a:visited { color:#888888; text-decoration:none; }
#HeaderSubMenu li a:hover,
#HeaderSubMenu li a:active { color:#EDEDED; background-color:#636363; }

#HeaderMenu { clear:both; height:40px; background:url("../images/header_menu__bg.gif") no-repeat 0 -0px; padding-left:0; }
#HeaderMenuFrame { background:none; padding-right:0; }
#HeaderMenuBody { background:none; }
#HeaderLogo { float:left; width:300px; height:50px; text-align:center; margin-top:-10px; }
#HeaderLogoFrame { padding:0; }
#HeaderMenu div.MenuTab { clear:none; float:right; height:28px; background-image:url("../images/header_menu__bg.gif"); padding-left:10px; margin-top:12px; }
#HeaderMenu div.MenuTab a { height:28px; color:#FFFFFF; background-image:url("../images/header_menu_left.gif"); background-position:0% -70px; }
#HeaderMenu div.MenuTab a span { line-height:28px; background-image:url("../images/header_menu_right.gif"); background-position:100% -70px; padding-left:5px; padding-right:10px; padding-top:0px; }
#HeaderMenu div.MenuTab a:hover { color:#FFFFFF; background-position:0% -105px; }
#HeaderMenu div.MenuTab a:hover span { background-position:100% -105px; }
#HeaderMenu div.MenuTab a:active,
#HeaderMenu div.MenuTab a:visited { color:#FFFFFF; }
#HeaderMenu div.MenuTab li.jut a { color:#FFFFFF; background-position:0% -105px; }
#HeaderMenu div.MenuTab li.jut a span { background-position:100% -105px; }
#HeaderMenu div.MenuTab li.jut a:hover { color:#FFFFFF; background-position:0% -70px; }
#HeaderMenu div.MenuTab li.jut a:hover span { background-position:100% -70px; }
#HeaderMenu div.MenuTab li.pop a { color:#FFFFFF; background-position:0% -35px; }
#HeaderMenu div.MenuTab li.pop a span { background-position:100% -35px; } 
#HeaderMenu div.MenuTab li.pop a:hover { color:#FFFFFF; background-position:0% -105px; }
#HeaderMenu div.MenuTab li.pop a:hover span { background-position:100% -105px; }
#HeaderMenu div.MenuTab li.space a { background-image:none; background-position:0 0; }
#HeaderMenu div.MenuTab li.space a span { padding:0; background-image:none; }
