#HeaderBox {  }
#HeaderBoxFrame {  }
#HeaderBoxHeader { display:none; }
#HeaderBoxFooter { display:none; }
#HeaderBoxBody { height:30px; background:url("../images/header_nav_bg.gif") repeat-x left top; padding-left:0; }
#HeaderBoxBody div.partInner { background:none; padding-right:0; }
#HeaderBoxBody div.partBody { background:none; }
#HeaderBoxBody div.partContent { clear:both; position:relative; height:30px; line-height:30px; color:#FFFFFF; font-size:13px; text-align:center; overflow:hidden; }
#HeaderBoxBody div.partContent .nav { height:30px; line-height:30px; }
#HeaderBoxBody div.partContent .nav { _padding-top:6px; }
#HeaderBoxBody div.partContent .nav-index { position:absolute; top:0; left:0; text-align:right; width:215px; padding-top:6px; }
#HeaderBoxBody div.partContent .nav-index { top:3px\9; _padding-top:3px; }
#HeaderBoxBody div.partContent .nav-left { position:absolute; top:0; left:220px; text-align:center; padding-top:6px; }
#HeaderBoxBody div.partContent .nav-left { padding-top:8px\9; _padding-top:6px; }
#HeaderBoxBody div.partContent .nav-right { position:absolute; top:0; right:0; text-align:right; margin-right:8px; padding-top:6px; }
#HeaderBoxBody div.partContent .nav-right { padding-top:9px\9; _padding-top:5px; }
#HeaderBoxBody div.partContent .loading { color:#CC6666; }
#HeaderBoxBody div.partContent a { line-height:normal; color:#FFFFFF; }
#HeaderBoxBody div.partContent a,
#HeaderBoxBody div.partContent a:visited { text-decoration:none; }
#HeaderBoxBody div.partContent a:hover,
#HeaderBoxBody div.partContent a:active { text-decoration:underline; }
#HeaderBoxBody div.partContent a.button { display:inline; border:0; background:url("../images/icon_menu.gif") 0 5px no-repeat; padding-left:15px; margin:0 1px 0 5px; }
#HeaderBoxBody div.partContent a.button { _background-position:0 3px; }
*+html #HeaderBoxBody div.partContent a.button { background-position:0 3px; }
#HeaderBoxBody div.partContent a.button-abc { _background-position:0 4px; }
*+html #HeaderBoxBody div.partContent a.button-abc { background-position:0 4px; }
#HeaderBoxBody div.partContent a.button-index { background-image:url("../images/icon_menu_index.gif"); }
#HeaderBoxBody div.partContent a.button-pop { background-image:url("../images/icon_menu_pop.gif"); }
#HeaderBoxBody div.partContent a:hover,
#HeaderBoxBody div.partContent a:active { text-decoration:underline!important; }

div.HeaderMenuBox { }
div.HeaderMenuBoxList { clear:both; border:1px solid #CE0020; background-color:#636363; padding:0; }
div.HeaderMenuBoxList ul { padding:0; }
div.HeaderMenuBoxList li { clear:both; height:25px; }
div.HeaderMenuBoxList li a { display:block; line-height:25px; padding:0 8px 0 8px; }
div.HeaderMenuBoxList li a,
div.HeaderMenuBoxList li a:visited { color:#EDEDED; text-decoration:none; background:#636363; }
div.HeaderMenuBoxList li a:hover,
div.HeaderMenuBoxList li a:active { color:#FFFFFF; text-decoration:none; background:#333333; }


